{
  "name": "rule34-vercel-backend",
  "version": "1.0.0",
  "description": "Vercel backend for Rule34 Mobile Downloader",
  "type": "module",
  "scripts": {
    "dev": "vercel dev",
    "deploy": "vercel --prod",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "vercel": "^33.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "api/**/*.js",
      "!api/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "keywords": [
    "proxy",
    "cors",
    "vercel",
    "serverless"
  ],
  "author": "",
  "license": "MIT"
}